{
  "sections": {
    "dbtfy-breadcrumbs":{
      "type":"dbtfy-breadcrumbs",
      "settings":{
        "breadcrumbs":true,
        "show_on":"both",
        "show_social_media_icons":"both",
        "share_on_facebook":true,
        "share_on_messenger":true,
        "tweet_on_twitter":true,
        "pin_on_pinterest":true,
        "share_on_whatsapp":true,
        "share_on_linkedin":true,
        "share_on_reddit":true,
        "share_on_email":true,
        "padding_top":20,
        "padding_bottom":20,
        "color_scheme":"scheme-2"
      }
    },
    "main": {
      "type": "main-article",
      "blocks": {
        "featured_image": {
          "type": "featured_image",
          "settings": {
            "image_height": "medium"
          }
        },
        "title": {
          "type": "title",
          "settings": {
            "blog_show_date": true,
            "blog_show_author": false
          }
        },
        "share": {
          "type": "share",
          "settings": {
            "share_label": "Share"
          }
        },
        "content": {
          "type": "content",
          "settings": {
          }
        }
      },
      "block_order": [
        "featured_image",
        "title",
        "share",
        "content"
      ],
      "settings": {
      }
    }
  },
  "order": [
    "dbtfy-breadcrumbs",
    "main"
  ]
}